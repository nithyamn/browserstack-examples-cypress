{
  "name": "cypress-support",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "npm run cypress:run 'cypress/integration/e2e/end_to_end.spec.ts'",
    "clean:reports": "rm -rf cypress/report/ && rm -rf cypress/videos/ && rm -rf cypress/screenshots/",
    "cypress:run": "npm run clean:reports && npx cypress run --config-file cypress.json --spec",
    "cypress:open": "cypress open",
    "lint": "npx eslint --ext .ts,.js .",
    "on-prem": "npm run cypress:run",
    "on-prem-default": "npm run cypress:run 'cypress/integration/e2e/end_to_end.spec.ts' --headed",
    "on-prem-suite": "npm run cypress:run",
    "docker": "npm run clean:reports && docker-compose up && docker-compose down",
    "docker-default": "npm run clean:reports && spec='cypress/integration/e2e/end_to_end.spec.ts' docker-compose up && docker-compose down",
    "bstack-single": "browserstack-cypress --config-file run_conf/bstack-single.json run --sync --spec",
    "bstack-single-default": "browserstack-cypress --config-file run_conf/bstack-single.json run --sync --spec 'cypress/integration/e2e/end_to_end.spec.ts'",
    "bstack-local": "browserstack-cypress --config-file run_conf/bstack-local-single.json run --sync --spec",
    "bstack-local-default": "browserstack-cypress --config-file run_conf/bstack-local-single.json run --sync --spec 'cypress/integration/e2e/end_to_end.spec.ts'",
    "bstack-local-parallel": "browserstack-cypress --config-file run_conf/bstack-local-single.json run --sync --parallels",
    "bstack-local-parallel-browsers": "browserstack-cypress --config-file run_conf/bstack-local-parallel.json run --sync --parallels",
    "bstack-parallel": "browserstack-cypress --config-file run_conf/bstack-single.json run --sync --parallels",
    "bstack-parallel-browsers": "browserstack-cypress --config-file run_conf/bstack-parallel.json run --sync --parallels",
    "percy-test": "npm run cypress:run 'cypress/integration/e2e/sample-percy.spec.ts' --headed"
  },
  "devDependencies": {
    "@cypress/react": "^5.12.5",
    "@cypress/webpack-dev-server": "^1.8.4",
    "@cypress/webpack-preprocessor": "~5.11.1",
    "@nrwl/cypress": "~14.1.6",
    "@percy/cli": "^1.10.2",
    "@percy/cypress": "^3.1.1",
    "@types/chance": "1.1.3",
    "@types/node": "~17.0.34",
    "browserstack-local": "~1.5.1",
    "chance": "~1.1.8",
    "cypress": "^9.6.1",
    "docker-compose": "~0.23.17",
    "eslint": "^8.15.0",
    "eslint-plugin-cypress": "~2.12.1",
    "mocha": "~10.0.0",
    "mochawesome": "~7.1.3",
    "ts-loader": "~9.3.0",
    "typescript": "4.6.4",
    "webpack": "5.72.1",
    "webpack-dev-server": "^4.9.0"
  },
  "dependencies": {
    "browserstack-cypress-cli": "~1.14.1",
    "sleep": "^6.3.0"
  }
}
